{
    "name"        : "jbzoo/toolbox",
    "description" : "It directly integrates the entire JBZoo libraries in one place. In the future, it will be the framework.",
    "license"     : "MIT",
    "keywords"    : ["jbzoo", "toolbox"],

    "authors"     : [
        {
            "name"  : "Denis Smetannikov",
            "email" : "admin@jbzoo.com",
            "role"  : "lead"
        }
    ],

    "require"     : {
        "php"               : ">=7.2",

        "jbzoo/assets"      : ">=4.3.2",
        "jbzoo/data"        : ">=4.3.3",
        "jbzoo/event"       : ">=3.2.2",
        "jbzoo/http-client" : ">=3.6.1",
        "jbzoo/image"       : ">=5.2.1",
        "jbzoo/less"        : ">=4.2.1",
        "jbzoo/mermaid-php" : ">=2.3.2",
        "jbzoo/path"        : ">=3.2.1",
        "jbzoo/utils"       : ">=4.5.4",
        "jbzoo/retry"       : ">=2.2.2",
        "jbzoo/markdown"    : ">=1.1.0",

        "guzzlehttp/guzzle" : ">=6.5.5"
    },

    "require-dev" : {
        "jbzoo/phpunit"             : ">=4.11.1",
        "jbzoo/codestyle"           : ">=3.0.0",
        "jbzoo/composer-graph"      : ">=1.3.2",
        "jbzoo/composer-diff"       : ">=1.5.2",
        "jbzoo/toolbox-dev"         : ">=3.1.0",
        "jbzoo/ci-report-converter" : ">=2.5.2",
        "jbzoo/jbdump"              : ">=1.5.6",

        "rmccue/requests"           : ">=1.8.1",
        "roave/security-advisories" : "dev-master"
    },

    "extra"       : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    }
}
